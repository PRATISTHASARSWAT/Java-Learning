public class Third {
    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);
        int i = sc.nextInt(); 

        // Q. FIBONACCI SERIES - Print the nth fibonacci number.
        int n = 5;

        int a = 0;
        int b = 1;
        int c = -1; //- to make a global variable.
        int cnt = 1;
        while(cnt <= n){
            c = a + b;
          //  System.out.println(c);
            a = b;
            b = c;

            cnt = cnt +1;
        }
        System.out.println(a);
        System.out.println(b);
        System.out.println(c);
    }
}
